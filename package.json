{
  "name": "jate",
  "version": "1.0.0",
  "description": "A PWA text editor application",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "build": "npm install --omit=dev && webpack --config client/webpack.config.js",
    "client": "npm run start --prefix cient",
    "server": "npm run start --prefix server",
    "install": "cd client && npm install && cd ../server && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [],
  "author": "Morgan",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
